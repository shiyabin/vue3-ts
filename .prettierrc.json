{
  "printWidth": 120,
  "singleQuote": true,
  "jsxBracketSameLine": true,
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "strict",
  "trailingComma": "none",
  "arrowParens": "avoid",
  "singleAttributePerLine": true,
  "endOfLine": "auto",
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "html"
      }
    }
  ]
}
